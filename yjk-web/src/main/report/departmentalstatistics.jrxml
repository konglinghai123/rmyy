	<queryString>
		<![CDATA[select o.name,sp.apply_start_date, sp.apply_end_date,
(select count(df.id)
from sb_drug_form df left join sp_system_parameter sp on df.system_parameter_id=sp.id
left join sec_user_organization_job uoj on df.user_id=uoj.user_id
where sp.is_enabled=true and o.id=uoj.organization_id and df.auditstatus = 'nodeclare'
) as nodeclare,
(select count(df.id)
from sb_drug_form df left join sp_system_parameter sp on df.system_parameter_id=sp.id
left join sec_user_organization_job uoj on df.user_id=uoj.user_id
where sp.is_enabled=true and o.id=uoj.organization_id and df.auditstatus = 'init'
) as init,
(select count(df.id)
from sb_drug_form df left join sp_system_parameter sp on df.system_parameter_id=sp.id
left join sec_user_organization_job uoj on df.user_id=uoj.user_id
where sp.is_enabled=true and o.id=uoj.organization_id and df.auditstatus = 'passed'
) as passed,
(select count(df.id)
from sb_drug_form df left join sp_system_parameter sp on df.system_parameter_id=sp.id
left join sec_user_organization_job uoj on df.user_id=uoj.user_id
where sp.is_enabled=true and o.id=uoj.organization_id and df.auditstatus = 'un_passed'
) as un_passed
from sec_organization o , sp_system_parameter sp
where o.parent_id != 0 and sp.is_enabled=true
order by o.weight asc]]>
	</queryString>
